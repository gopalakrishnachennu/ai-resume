{
    "manifest_version": 3,
    "name": "JobFiller Pro",
    "version": "2.0.0",
    "description": "Auto-fill job applications instantly with AI-powered suggestions",
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "https://www.linkedin.com/*",
        "https://linkedin.com/*",
        "https://jobs.lever.co/*",
        "https://boards.greenhouse.io/*",
        "https://www.greenhouse.io/*",
        "https://www.indeed.com/*",
        "https://indeed.com/*",
        "https://www.glassdoor.com/*",
        "https://glassdoor.com/*",
        "https://www.ziprecruiter.com/*",
        "https://www.monster.com/*",
        "https://careers.google.com/*",
        "https://jobs.ashbyhq.com/*",
        "https://jobs.smartrecruiters.com/*",
        "https://apply.workable.com/*",
        "https://boards.eu.greenhouse.io/*",
        "https://app.breezy.hr/*",
        "https://wellfound.com/*",
        "https://angel.co/*",
        "https://www.workatastartup.com/*",
        "https://ai-resume-gopalakrishnachennu-5461s-projects.vercel.app/*"
    ],
    "background": {
        "service_worker": "src/background/service-worker.ts",
        "type": "module"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.linkedin.com/*",
                "https://linkedin.com/*",
                "https://jobs.lever.co/*",
                "https://boards.greenhouse.io/*",
                "https://www.greenhouse.io/*",
                "https://www.indeed.com/*",
                "https://indeed.com/*",
                "https://www.glassdoor.com/*",
                "https://glassdoor.com/*",
                "https://www.ziprecruiter.com/*",
                "https://www.monster.com/*",
                "https://careers.google.com/*",
                "https://jobs.ashbyhq.com/*",
                "https://jobs.smartrecruiters.com/*",
                "https://apply.workable.com/*",
                "https://boards.eu.greenhouse.io/*",
                "https://app.breezy.hr/*",
                "https://wellfound.com/*",
                "https://angel.co/*",
                "https://www.workatastartup.com/*",
                "https://ai-resume-gopalakrishnachennu-5461s-projects.vercel.app/*"
            ],
            "js": [
                "src/content/index.ts"
            ],
            "run_at": "document_idle"
        }
    ],
    "action": {
        "default_popup": "src/ui/popup/popup.html",
        "default_icon": {
            "16": "public/icons/icon16.png",
            "48": "public/icons/icon48.png",
            "128": "public/icons/icon128.png"
        }
    },
    "icons": {
        "16": "public/icons/icon16.png",
        "48": "public/icons/icon48.png",
        "128": "public/icons/icon128.png"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "assets/*",
                "public/*"
            ],
            "matches": [
                "https://www.linkedin.com/*",
                "https://jobs.lever.co/*",
                "https://boards.greenhouse.io/*",
                "https://www.indeed.com/*",
                "https://www.glassdoor.com/*",
                "https://www.ziprecruiter.com/*",
                "https://www.monster.com/*",
                "https://careers.google.com/*",
                "https://jobs.ashbyhq.com/*",
                "https://jobs.smartrecruiters.com/*",
                "https://apply.workable.com/*",
                "https://ai-resume-gopalakrishnachennu-5461s-projects.vercel.app/*"
            ]
        }
    ]
}